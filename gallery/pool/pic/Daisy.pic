scale_x = 0.1;
scale_y = 0.1;
scale_z = 0.1;
radius=9.0;
illumination = ambient_light + diffuse_light + reflected_light + transmitted_light;
ambient      = 40;
diffuse      = 80;
reflected    = 80;
transmitted  = 20;
smoothness   = 50;
transparence = 0;
thickness = 10;
antialiasing = 6;

background_red=255;background_blue=255;background_green=255;//=0; background_green=0; background_blue=51;

poly f1 = (x^2-1*y^3)^2 -1* (z^2-1*y^2)^3;
surface = f1;
surface_red = 255;surface_green = 153;surface_blue = 51;
inside_red = 204;inside_green = 255;inside_blue = 153;
transparence = 0;
surface=rotate(surface,-1.3429291839732782,yAxis);
surface=rotate(surface,0.510687964911918,xAxis);
surface=rotate(surface,1.6845822779759674,zAxis);

draw_surface;
filename="Daisy.ppm";
color_file_format=ppm;
save_color_image;
